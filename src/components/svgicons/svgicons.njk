{% for c in category %}

<h3>{{ c.name }}</h3>
<table class="icon-table usa-table-borderless">
  <thead>
    <tr>
        <th scope="col">Icon</th>
        <th scope="col">ID</th>
        <th scope="col">Usage</th>
    </tr>
  </thead>
  <tbody>
  {% for i in c.icon %}
    <tr>
        <th scope="row">icon here {{ i.id }}</th>
        <td>{{ i.id }} | {{ i.html }}</td>
        <td>{{ i.usage }}</td>
    </tr>
  {% endfor %}
</tbody>
</table>
{% endfor %}
